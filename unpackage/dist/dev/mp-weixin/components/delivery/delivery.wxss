.delivery_t {
  display: flex;
  flex-direction: column;
  width: 94%;
  background-color: white;
  margin-top: 10rpx;
  margin-left: 3%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.delivery_t .t_item {
  width: 90%;
  height: 100rpx;
  display: flex;
  background-color: #efefef;
  border: 1rpx solid #efefef;
  border-radius: 30rpx;
  align-content: center;
  justify-content: space-between;
  align-items: center;
  margin-top: 15rpx;
  margin-left: 4.5%;
}
.delivery_t .t_item .item_list {
  display: flex;
  padding-left: 10rpx;
}
.delivery_t .t_item .item_md {
  font-size: 26rpx;
  padding-right: 10rpx;
}
.delivery_t .add_address {
  color: #808069;
  font-size: 22rpx;
  padding-left: 20rpx;
  line-height: 60rpx;
}
.article_info {
  display: flex;
  width: 94%;
  background-color: white;
  margin-left: 3%;
  flex-direction: column;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.article_info .info_t {
  height: 60rpx;
  width: 100%;
  background-color: #b0e0e6;
  border: 1px solid #b0e0e6;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  display: flex;
  align-items: center;
  color: white;
}
.article_info .info_t text {
  padding-left: 3%;
}
.article_info .add_annex {
  display: flex;
  height: 120rpx;
  width: 95%;
  background-color: #efefef;
  padding-top: 10rpx;
  margin-left: 2.5%;
  margin-top: 10rpx;
  flex-direction: column;
  font-size: 26rpx;
  border: 1px solid #efefef;
  border-radius: 30rpx;
}
.article_info .add_annex .add_t {
  height: 50rpx;
  width: 100%;
  display: flex;
}
.article_info .add_annex .add_t .add_icon {
  display: flex;
}
.article_info .add_annex .add_t .add_icon image {
  width: 55rpx;
  height: 50rpx;
  margin-left: 15rpx;
}
.article_info .add_annex .add_t .add_icon text {
  font-size: 22rpx;
  color: #808069;
  padding-top: 10rpx;
}
.article_info .add_annex .download_annex {
  height: 50rpx;
  width: 25%;
  display: flex;
  align-self: flex-end;
  margin-top: 20rpx;
}
.article_info .add_annex .download_annex text {
  font-size: 20rpx;
  padding-top: 10rpx;
  font-size: 26rpx;
}
.article_info .add_annex .download_annex image {
  width: 50rpx;
  height: 50rpx;
}
.article_info .info_list {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 94%;
  margin-left: 3%;
  font-size: 26rpx;
}
.article_info .info_list .info_item {
  height: 65rpx;
  display: flex;
  align-items: center;
  margin-top: 10rpx;
  border-bottom: 2rpx solid #efefef;
}
.article_info .info_list .info_item input {
  font-size: 26rpx;
}